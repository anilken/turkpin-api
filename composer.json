{
    "name": "anilken/turkpin-api",
    "description": "Turkpin API Package",
    "keywords": [
        "php",
        "anilken",
        "turkpin-api",
        "turkpin.com",
        "turkpin"
    ],
    "homepage": "https://github.com/anilken/turkpin-api",
    "license": "MIT",
    "authors": [
        {
            "name": "ANILKEN",
            "email": "info@anilcanaslan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Anilken\\Turkpin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anilken\\Turkpin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
